$TTL 3h

@ IN SOA nameserver1.intranet.local. intranet.local. (

	1 ; Serial
	604800 ; Refresh
	86400 ; Retry
	2419200 ; Expire
	604800 ; Negative Cache TTL

)

; NAMESERVER DEFINITIONS
@ IN NS nameserver1.intranet.local.
@ IN NS nameserver2.intranet.local.

; POINTER RECORDS
100 IN PTR nameserver1.intranet.local.
101 IN PTR nameserver2.intranet.local.
